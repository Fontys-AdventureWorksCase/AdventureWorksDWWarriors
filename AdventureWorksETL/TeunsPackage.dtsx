                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">select 
soh.SalesOrderID,
sod.SalesOrderDetailID,
prd.ProductID,
prd.Name as Product,
mod.Name as Model,
cat.Name as Category,
scat.Name as SubCat,
sod.OrderQty,
soh.OrderDate,
soh.OnlineOrderFlag,
adr.City as ShipToCity,
prv.StateProvinceCode as ShipToProvinceCode,
prv.Name as ShipToProvinceName,
cnt.CountryRegionCode as ShipToCountryCode,
cnt.Name as ShipToCountryName
from Sales.SalesOrderDetail sod
join Sales.SalesOrderHeader soh on sod.SalesOrderID=soh.SalesOrderID
join Person.Address adr on adr.AddressID=soh.ShipToAddressID
join Person.StateProvince prv on prv.StateProvinceID=adr.StateProvinceID
join Person.CountryRegion cnt on cnt.CountryRegionCode=prv.CountryRegionCode
join Production.Product prd on sod.ProductID=prd.ProductID
join Production.ProductModel mod on prd.ProductModelID=mod.ProductModelID
join Production.ProductSubcategory scat on scat.ProductSubcategoryID=prd.ProductSubcategoryID
join Production.ProductCategory cat on cat.ProductCategoryID=scat.ProductCategoryID</property>
